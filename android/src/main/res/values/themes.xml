<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LoginWebViewTheme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="appBarLayoutStyle">@style/AppTheme.AppBarOverlay</item>
        <item name="themeLoginToolbar">@style/LoginToolbarTheme</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="Widget.Design.AppBarLayout">
        <item name="android:backgroundTint">@color/loginToolbarBackground</item>
        <item name="elevation">0dp</item>
    </style>

    <style name="LoginToolbarTheme" parent="Theme.AppCompat">
        <item name="android:textColorPrimary">@color/loginToolbarOnBackground</item>
        <item name="colorCloseIcon">@color/loginToolbarOnBackground</item>
    </style>
</resources>
